{% extends "_layouts/site" %}
{% set title = entry.title %}
{% set bodyClass = 'body--grey' %}

{% block content %}
<div class="generic-wrapper">
	<h1 class="generic__title">{{ entry.title }}</h1>

	<div class="generic__article">
		{% if entry.pullQuote %}
			<div class="pullQuote">{{ entry.pullQuote }}</div>
		{% endif %}
		{{ entry.body }}
	</div>
</div>
{% endblock %}
