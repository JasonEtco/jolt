{
  'parser': 'babel-eslint',
  'extends': 'airbnb',

  'globals': {
    'zxcvbn': true
  },

  'parserOptions': {
    'ecmaFeatures': {
      'experimentalObjectRestSpread': true
    }
  },

  'rules': {
    'no-underscore-dangle': [2, { "allow": ['_id'] }],
    'new-cap': 1,
    'max-len': 1,
  }
}
